
@{
    ViewData["Title"] = "My Returned Cases";
}


<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>My Case</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">My Returned Collaterals List</li>
                </ol>
            </div>
        </div>
    </div>
</section>
<section class="content">
    <div class="container-fluid">
        <div class="card">
            <div class="card-body">
                <table id="MOMyCasesDatatable" class="table table-bordered table-striped">
                    <thead>
                        <tr>
                            <th>PropertyOwner</th>
                            <th>Role </th>
                            <th>Category</th>
                            <th>Type</th>
                            <th>Action</th>
                        </tr>
                    </thead>

                </table>
            </div>
        </div>
    </div>
</section>
@section Scripts {
    @{
        await Html.RenderPartialAsync("ScriptPartial/MO/_MOMyReturnedCollateralsDatatableSriptsPartial");
    }
}
