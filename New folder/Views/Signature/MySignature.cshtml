
@{
    ViewData["Title"] = "My Signature";
}

<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>My Case</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">My Signature</li>
                </ol>
            </div>
        </div>
    </div>
</section>
<section class="content">
    <div class="container-fluid">
        <div class="card">
            <div class="card-body">
                <table id="mysignatureDatatable" class="table table-bordered table-striped">
                    <thead>
                        <tr>
                            <th>My Employee ID</th>
                            <th>Created At</th>
                            <th>My signature</th>
                       </tr>
                    </thead>

                </table>
            </div>
        </div>
    </div>
</section>
@section Scripts {
    @{
       
        await Html.RenderPartialAsync("ScriptPartial/_ValidationScriptsPartial");
        await Html.RenderPartialAsync("ScriptPartial/_MySignatureScriptPartial");
    }
}
