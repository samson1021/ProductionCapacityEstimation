@model mechanical.Models.Dto.IndBldgFacilityEquipmentDto.IndBldgFacilityEquipmentPostDto;
@{
    var collateral = ViewData["collateral"] as ReturnCollateralDto;
    var collateralFiles = ViewData["collateralFile"] as IEnumerable<ReturnFileDto>;
}
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6 col-xl-8">
                <h3 class="m-0">Industrial (Mfg) & Building Facility Machinery Type Evaluation Form</h3>
            </div>
            <div class="col-sm-6 col-xl-4">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">My Case</li>
                    <li class="breadcrumb-item active">Detail</li>
                    <li class="breadcrumb-item active">Collateral evaluation</li>
                </ol>
            </div>
        </div>
    </div>
</div>
<section class="content">
    <section class="col-xl-12 connectedSortable">
        <div class="card">
            <div class="card-body">
                <form method="post" enctype="multipart/form-data" asp-action="Create">
                    <input asp-for="CollateralId" value=@collateral.Id type="hidden" />
                    <div class="row">
                        <div class="form-group col-xl-4">
                            <label asp-for="IndustrialBuildingMachineryType">Indu(Mfg) & Bud Facility Machinery Type</label>
                            <select asp-for="IndustrialBuildingMachineryType" class="form-control"
                                    asp-items="@(new SelectList(Enum.GetValues(typeof(IndustrialBuildingMachineryType)).Cast<IndustrialBuildingMachineryType>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                <option value="" disabled selected>Select Indu(Mfg) & Bud Facility Machinery Type</option>
                            </select>
                            <span asp-validation-for="IndustrialBuildingMachineryType" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-4">
                            <label asp-for="EngineType"> Power Source & Fuel Type</label>
                            <select asp-for="EngineType" class="form-control"
                                    asp-items="@(new SelectList(Enum.GetValues(typeof(EngineType)).Cast<EngineType>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                <option value="" disabled selected> Select Power Source & Fuel Type </option>
                            </select>
                            <span asp-validation-for="EngineType" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-4">
                            <label asp-for="PowerSupply">Power Supply</label>
                            <select asp-for="PowerSupply" class="form-control"
                                    asp-items="@(new SelectList(Enum.GetValues(typeof(PowerSupply)).Cast<PowerSupply>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                <option value="" disabled selected>Select Power Supply</option>
                            </select>
                            <span asp-validation-for="PowerSupply" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-xl-4">
                            <label asp-for="MotorPower">Motor Power (Cc/Kw/Hp)</label>
                            <input asp-for="MotorPower" type="text" class="form-control" id="enginePower" placeholder="Motor Power (Cc/Kw/Hp)" required>
                            <span asp-validation-for="MotorPower" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-4">
                            <label asp-for="WorkingProductionCapacity">Working/Production Capacity</label>
                            <input asp-for="WorkingProductionCapacity" type="text" class="form-control" id="enginePower" placeholder="Working/Production Capacity" required>
                            <span asp-validation-for="WorkingProductionCapacity" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-4">
                            <label asp-for="OtherTechSpec">Insert Other Tech. Spec.(if any)</label>
                            <input asp-for="OtherTechSpec" type="text" class="form-control" id="OtherTechSpec" placeholder="Insert Other Tech. Spec.(if any)" required>
                            <span asp-validation-for="OtherTechSpec" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-xl-4">
                            <label asp-for="MakerCompany" >Insert Maker Company </label>
                            <input asp-for="MakerCompany" type="text" class="form-control" id="MakerCompany" placeholder="Insert Other Tech. Spec.(if any)" required oninput="validateMakeCompanyInput(this)" >
                            <span id="Make-error" asp-validation-for="MakerCompany" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-4">
                            <label asp-for="TechnologyStandard">Technology Standard</label>
                            <select asp-for="TechnologyStandard" class="form-control"
                                    asp-items="@(new SelectList(Enum.GetValues(typeof(MachineryTechnologyStandard)).Cast<MachineryTechnologyStandard>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                <option value="" disabled selected>Select Technology Standard</option>
                            </select>
                            <span asp-validation-for="TechnologyStandard" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-4">
                            <label asp-for="MakerPreferenceType">Type of Maker Preference</label>
                            <select asp-for="MakerPreferenceType" class="form-control" id="enginePower" required>
                                <option value="">Select the preference</option>
                                <option value="Normal Preference">Normal Preference</option>
                                <option value="Exceptional Preference">Exceptional Preference</option>
                            </select>
                            <span asp-validation-for="MakerPreferenceType" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-xl-4">
                            <label asp-for="CurrentEqpmntCondition">Current Equipment Condition</label>
                            <select asp-for="CurrentEqpmntCondition" class="form-control"
                                    asp-items="@(new SelectList(Enum.GetValues(typeof(EquipmentCondition)).Cast<EquipmentCondition>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                <option value="" disabled selected>Select Current Equipment Condition</option>
                            </select>
                            <span asp-validation-for="CurrentEqpmntCondition" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-4">
                            <label asp-for="AllocatedPointsRange">Allocated Points Range</label>
                            <select asp-for="AllocatedPointsRange" class="form-control"
                                    asp-items="@(new SelectList(Enum.GetValues(typeof(AllocatedPointRange)).Cast<AllocatedPointRange>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                <option value="" disabled selected>Select Current Equipment Condition</option>
                            </select>
                            <span asp-validation-for="AllocatedPointsRange" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-4">
                            <label asp-for="ModelNo">Model Number</label>
                            <input asp-for="ModelNo" id="ModelNo" type="text" value="@collateral.ModelNo" class="form-control" placeholder="Model Number" required oninput="validateModelNoInput(this)">

                            <span id="model-error" asp-validation-for="ModelNo" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row">
                       <div class="form-group col-xl-4">
                            <label asp-for="EngineNo">Motor Number</label>
                            <input asp-for="EngineNo" id="EngineNo" type="text" value="@collateral.EngineMotorNo" class="form-control" placeholder="Engine Number" required oninput="validateMotorNoInput(this)">
                            <span id="motor-error" asp-validation-for="EngineNo" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-4">
                            <label asp-for="SerialNo">Serial Number</label>
                            <input asp-for="SerialNo" id="SerialNo" type="text" value="@collateral.SerialNo" class="form-control" placeholder="Serial Number" required oninput="validateSerialNoInput(this)">
                            <span id="serial-error" asp-validation-for="SerialNo" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-4">
                            <label asp-for="YearOfManufacture">Year of Manufacture</label>
                            <input asp-for="YearOfManufacture" value="@collateral.ManufactureYear" type="number" class="form-control" min="1900" max="2099" step="1" required>
                            <span asp-validation-for="YearOfManufacture" class="text-danger"></span>
                        </div>
                    </div>
                     <div class="row">
                        <div class="form-group col-xl-4">
                            <label asp-for="Remark">Remark</label>
                            <textarea asp-for="Remark" class="form-control" placeholder="Remark" rows="3" required></textarea>
                            <span asp-validation-for="Remark" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-4">
                            <label asp-for="PhysicalAndInstallationAssesment">Physical and Installation Assessment</label>
                            <textarea asp-for="PhysicalAndInstallationAssesment" class="form-control" rows="3" placeholder="Physical and Installation Assessment" required></textarea>
                            <span asp-validation-for="PhysicalAndInstallationAssesment" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-4">
                            <label asp-for="OverallSurveyAssesment">Overall Survey Assessment</label>
                            <textarea asp-for="OverallSurveyAssesment" class="form-control" placeholder="Overall Survey Assessment" rows="3" required></textarea>
                            <span asp-validation-for="OverallSurveyAssesment" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-xl-3">
                            <label asp-for="CountryOfOrigin">Country of Origin</label>
                            <input asp-for="CountryOfOrigin" type="text" class="form-control" placeholder="Select Country of Origin" list="country" required>
                            <span asp-validation-for="CountryOfOrigin" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-3">
                            <label asp-for="InvoiceValue">Invoice Value</label>
                            <input asp-for="InvoiceValue" type="number" class="form-control" placeholder="Invoice Value" required>
                            <span asp-validation-for="InvoiceValue" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-3">
                            <label asp-for="Currency">Currency</label>
                            <input asp-for="Currency" list="currencyList" type="text" class="form-control" placeholder="Select Currency" required>
                            <span asp-validation-for="Currency" class="text-danger"></span>
                        </div>
                        <div class="form-group col-xl-3">
                            <label asp-for="ExchangeRate">Exchange Rate</label>
                            <input asp-for="ExchangeRate"  type="number" class="form-control" placeholder="Enter Exchange Rate" required>
                            <span asp-validation-for="ExchangeRate" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6 ">
                            <button type="submit" class="btn bg-purple col-xl-3">Save</button>
                        </div>
                        <div class="col-md-6 ">
                            <button type="button" id="RejectButton" class="btn btn-sm btn-danger col-3" data-toggle="modal" data-target="#rejectionModal1">
                                Return <i class="fas fa-backward"></i>
                            </button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </section>
</section>
@section Scripts {
    <script>
        $(function () {
            $('#rejectionModal1').on('shown.bs.modal', function () {
                $('#rejectionComment').focus();
            });

            $('#rejectionForm').submit(function (event) {
                event.preventDefault(); // Prevent default form submission

                var rejectionComment = $('#rejectionComment').val();
                var CollateralId = $('#CollateralId').val();

                $.ajax({
                    url: $(this).attr('action'),
                    type: $(this).attr('method'),

                    data: { rejectionComment: rejectionComment, CollateralId: CollateralId },
                    success: function (response) {
                        // Handle the response from the controller if needed
                        toastr.success(' Reason for Return saved successfully');
                        $('#rejectionModal1').modal('hide');
                        window.location.href = '/MOCase/MyCases';

                    },
                    error: function (error) {
                        // Handle the error if needed
                        toastr.error('Error saving rejection comment:', error);
                    }
                });
            });
        });
    </script>
    @{
        await Html.RenderPartialAsync("ScriptPartial/_MVSriptsPartial");
        await Html.RenderPartialAsync("ScriptPartial/_ValidationScriptsPartial");
    
    }
}
<script>
 
    //this is to validate all collateral addresses
    function validateMakeCompanyInput(inputElement) {
        var makeError = document.getElementById("Make-error");
        var makeValue = inputElement.value;
        console.log(makeValue);
        var alphanumericRegex = /^[a-zA-Z]*$/;
        if (makeValue.length > 0 && !alphanumericRegex.test(makeValue)) {
            makeError.textContent = "Maker Company must contain only alphabet characters.";
        } else {
            makeError.textContent = ""; // Clear the error message if valid
        }
    }
    function validateModelNoInput(inputElement) {
        var modelError = document.getElementById("model-error");
        var modelValue = inputElement.value;

        var alphanumericRegex = /^[a-zA-Z0-9]*$/;
        if (modelValue.length > 0 && !alphanumericRegex.test(modelValue)) {
            modelError.textContent = "Model No contain only alphanumeric characters.";
