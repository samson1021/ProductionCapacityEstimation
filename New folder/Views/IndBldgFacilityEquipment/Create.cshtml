        var motorerror = document.getElementById("motor-error");
        var motorValue = inputElement.value;

        var alphanumericRegex = /^[a-zA-Z0-9\s]*$/;
        if (motorValue.length > 0 && !alphanumericRegex.test(motorValue)) {
            motorerror.textContent = "Motor No must contain only alphanumeric characters.";
        } else {
            motorerror.textContent = ""; // Clear the error message if valid
        }
    }
    function validateSerialNoInput(inputElement) {
        var serialerror = document.getElementById("serial-error");
        var SerialValue = inputElement.value;

        var alphanumericRegex = /^[a-zA-Z0-9\s]*$/;
        if (SerialValue.length > 0 && !alphanumericRegex.test(SerialValue)) {
            serialerror.textContent = "Wereda must contain only alphanumeric characters.";
        } else {
            serialerror.textContent = ""; // Clear the error message if valid
