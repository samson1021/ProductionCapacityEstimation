@{
    ViewData["Title"] = ViewBag.Title as string;
}

<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h3>My PCE Case Assignments</h3>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="@Url.Action("Index", "Home")">Home</a></li>
                    <li class="breadcrumb-item active">My PCE Case Assignments</li>
                </ol>
            </div>
        </div>
    </div>
</section>
<section class="content">
    <div class="container-fluid">
        <div class="card">
            <div class="card-body">
                <table id="MyPCECasesDatatable" class="table table-bordered table-striped">
                    <thead>
                        <tr>
                            <th>Created At</th>
                            <th>Case No</th>
                            @* <th>Requesting Unit</th> *@
                            <th>Applicant Name</th>
                            <th>Customer Id</th>
                            <th>New Productions</th>
                            <th>Total Productions</th>
                            <th>Action</th>
                        </tr>
                    </thead>

                </table>
            </div>
        </div>
    </div>
</section>

@section Scripts {
    @{
        await Html.RenderPartialAsync("PCEScriptPartial/Partial/_CommonLibrariesScriptPartial");
        await Html.RenderPartialAsync("Partial/_MyAssignmentsDatatableSriptsPartial");
    }
}
