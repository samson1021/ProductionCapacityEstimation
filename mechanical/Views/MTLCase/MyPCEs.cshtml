﻿@{
    ViewData["Title"] = ViewBag.Title as string;
}

<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h4>@ViewData["Title"]</h4>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="@Url.Action("Index", "Home")">Home</a></li>
                    <li class="breadcrumb-item"><a href="@Url.Action("MyPCECases", "MMCase")">My PCE Cases</a></li>
                    <li class="breadcrumb-item active">@ViewData["Title"]</li>
                </ol>
            </div>
        </div>
    </div>
</section>
<section class="content">
    <div class="container-fluid">
        @await Html.PartialAsync("_PCEsTable")
    </div>
</section>

@section Scripts {
    @{
        await Html.RenderPartialAsync("PCEScriptPartial/_RejectModal");
        await Html.RenderPartialAsync("Partial/_PCEsTableDatatableSriptsPartial");
        await Html.RenderPartialAsync("PCEScriptPartial/Partial/_CommonLibrariesScriptPartial");
        await Html.RenderPartialAsync("ScriptPartial/_ValidationScriptsPartial");
    }   
}
