                        @await Html.PartialAsync("Partial/_PCEGeneralInfo")
                    </div>
                   <div class="tab-pane fade show active" id="custom-tabs-four-profile" role="tabpanel" aria-labelledby="custom-tabs-four-profile-tab">
                        @await Html.PartialAsync("Partial/_PCECreateCollateral")
                    </div>
                      <div class="tab-pane fade" id="custom-tabs-four-messages" role="tabpanel" aria-labelledby="custom-tabs-four-messages-tab">
                        @await Html.PartialAsync("Partial/_PCECaseSchedule")
                    </div>@* *@
                    <div class="tab-pane fade" id="custom-tabs-four-settings" role="tabpanel" aria-labelledby="custom-tabs-four-settings-tab">
                    @await Html.PartialAsync("Partial/_PCECaseComment")
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
