<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h4>Production Capacity Estimation Case Detail </h4>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active"><a href="@Url.Action("PCENewCases", "PCECase")">My Case</a></li>
                    <li class="breadcrumb-item active">PCE Case Detail</li>
                </ol>
            </div>
