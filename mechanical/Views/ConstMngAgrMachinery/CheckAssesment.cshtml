@model mechanical.Models.Dto.ConstMngAgrMachineryDto.ConstMngAgMachineryReturnDto;
@{

    var constMngAgMachinery = ViewData["constMngAgMachinery"] as ConstMngAgrMachinery;
}
<div class="modal fade" id="CheckAssesmentModal" tabindex="-1" role="dialog" aria-labelledby="CheckAssesmentModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="CollateralFileModalLabel">   <img src="~/img/assignment_add_FILL0_wght300_GRAD200_opsz20.svg" class="nav-icon" alt="Custom Icon">Construction, Minning & Agricultural Machinery  Evaluation Form</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <section class="col-xl-12 connectedSortable">
                <div class="card">
                    <div class="card-body">
                        <form method="post" id="checkAssessmentForm" enctype="multipart/form-data" asp-action="CheckAssessment" asp-route-Id="@Model.Id">
                            <input  asp-for="CollateralId" value=@Model.CollateralId type="hidden" />
                            <div class="row">
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="constructionMiningAgriculturalMachineryType"> Cons, Min & Agri Machinery Type</label>
                                        <select name="constructionMiningAgriculturalMachineryType" asp-for="@constMngAgMachinery.constructionMiningAgriculturalMachineryType" class="form-control form-control-sm"
                                                asp-items="@(new SelectList(Enum.GetValues(typeof(ConstructionMiningAgriculturalMachineryType)).Cast<ConstructionMiningAgriculturalMachineryType>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                            <option value="" disabled>Select Cons, Min & Agri Machinery Type</option>
                                        </select>
                                        <span asp-validation-for="constructionMiningAgriculturalMachineryType" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="EngineType">Engine/Motor Type</label>
                                        <select name="EngineType" asp-for="@constMngAgMachinery.EngineType" class="form-control form-control-sm"
                                                asp-items="@(new SelectList(Enum.GetValues(typeof(EngineType)).Cast<EngineType>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                            <option value="" disabled>Select Engine/Motor Type</option>
                                        </select>
                                        <span asp-validation-for="EngineType" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="PowerSupply">Power Supply</label>
                                        <select name="PowerSupply" asp-for="@constMngAgMachinery.PowerSupply" class="form-control form-control-sm"
                                                asp-items="@(new SelectList(Enum.GetValues(typeof(PowerSupply)).Cast<PowerSupply>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                            <option value="" disabled>Select Power Supply</option>
                                        </select>
                                        <span asp-validation-for="PowerSupply" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xl-4">
                                    <label asp-for="NoOfCylinder"> No. Cylinder</label>
                                    <select name="NoOfCylinder" asp-for="@constMngAgMachinery.NoOfCylinder " class="form-control"
                                            asp-items="@(new SelectList(Enum.GetValues(typeof(NoOfCylinder)).Cast<NoOfCylinder>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString(), Selected = ( e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name == Model.NoOfCylinder.ToString()) }), "Value", "Text"))">
                                    </select>
                                    <span asp-validation-for="NoOfCylinder" class="text-danger"></span>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="TransmissionType">Transmission Type</label>
                                        <select name="TransmissionType" asp-for="@constMngAgMachinery.TransmissionType" class="form-control form-control-sm"
                                                asp-items="@(new SelectList(Enum.GetValues(typeof(TransmissionType)).Cast<TransmissionType>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                            <option value="" disabled>Select Transmission Type</option>
                                        </select>
                                        <span asp-validation-for="TransmissionType" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="IgnitionSystem">Ignition System </label>
                                        <select name="IgnitionSystem" asp-for="@constMngAgMachinery.IgnitionSystem" class="form-control form-control-sm"
                                                asp-items="@(new SelectList(Enum.GetValues(typeof(IgnitionSystem)).Cast<IgnitionSystem>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                            <option value="" disabled>Select Ignition System</option>
                                        </select>
                                        <span asp-validation-for="IgnitionSystem" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="CoolingType">Cooling System</label>
                                        <select name="CoolingType" asp-for="@constMngAgMachinery.CoolingType" class="form-control form-control-sm"
                                                asp-items="@(new SelectList(Enum.GetValues(typeof(CoolingSystem)).Cast<CoolingSystem>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                            <option value="" disabled>Select Cooling System</option>
                                        </select>
                                        <span asp-validation-for="CoolingType" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="EnginePower">Engine Power(Cc/Kw/Hp)</label>
                                        <input name="EnginePower" asp-for="@constMngAgMachinery.EnginePower" type="text" class="form-control form-control-sm" id="enginePower" placeholder="Engine Power(Cc/Kw/Hp)" required>
                                        <span asp-validation-for="EnginePower" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="WorkingProductionCapacity">Working/Production Capacity</label>
                                        <input name="WorkingProductionCapacity" asp-for="@constMngAgMachinery.WorkingProductionCapacity" type="text" class="form-control form-control-sm" id="enginePower" placeholder="Working/Production Capacity" required>
                                        <span asp-validation-for="WorkingProductionCapacity" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="TechnologyStandard">Technology Standard</label>
                                        <select name="TechnologyStandard" asp-for="@constMngAgMachinery.TechnologyStandard" class="form-control form-control-sm"
                                                asp-items="@(new SelectList(Enum.GetValues(typeof(MachineryTechnologyStandard)).Cast<MachineryTechnologyStandard>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                            <option value="" disabled>Select Technology Standard</option>
                                        </select>
                                        <span asp-validation-for="TechnologyStandard" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="MakerPreferenceType">Type of Maker Preference</label>
                                        <select name="MakerPreferenceType" asp-for="@constMngAgMachinery.MakerPreferenceType" class="form-control form-control-sm" id="enginePower" required>
                                            <option value="">Select the preference</option>
                                            <option value="Normal Preference">Normal Preference</option>
                                            <option value="Exceptional Preference">Exceptional Preference</option>
                                        </select>
                                        <span asp-validation-for="MakerPreferenceType" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group  form-group-sm col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="CabinType">Cabin Type</label>
                                        <select name="CabinType" asp-for="@constMngAgMachinery.CabinType" class="form-control form-control-sm"
                                                asp-items="@(new SelectList(Enum.GetValues(typeof(CabinType)).Cast<CabinType>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                            <option value="" disabled>Select Cabin Type</option>
                                        </select>
                                        <span asp-validation-for="CabinType" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="NumberOfAxle">Number of Axles</label>
                                        <select name="NumberOfAxle" asp-for="@constMngAgMachinery.NumberOfAxle" class="form-control form-control-sm"
                                                asp-items="@(new SelectList(Enum.GetValues(typeof(NoOfAxles)).Cast<NoOfAxles>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                            <option value="" disabled>Select Number of Axles</option>
                                        </select>
                                        <span asp-validation-for="NumberOfAxle" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="Color">Color</label>
                                        <input name="Color" asp-for="@constMngAgMachinery.Color" list="carColors" type="text" class="form-control form-control-sm" placeholder="Select Car Color" required>
                                        <span asp-validation-for="Color" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="MakerCompany">Insert Maker Company </label>
                                        <input name="MakerCompany" asp-for="@constMngAgMachinery.MakerCompany" type="text" class="form-control form-control-sm" id="MakerCompany" placeholder="Insert Maker Company" required>
                                        <span asp-validation-for="MakerCompany" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group  col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="CurrentEqpmntCondition">Current Equipment Condition</label>
                                        <select name="CurrentEqpmntCondition" asp-for="@constMngAgMachinery.CurrentEqpmntCondition" class="form-control form-control-sm"
                                                asp-items="@(new SelectList(Enum.GetValues(typeof(EquipmentCondition)).Cast<EquipmentCondition>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString() }), "Value", "Text"))">
                                            <option value="" disabled>Select Current Equipment Condition</option>
                                        </select>
                                        <span asp-validation-for="CurrentEqpmntCondition" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <label asp-for="AllocatedPointsRange">Allocated Points Range</label>
                                    <select name="AllocatedPointsRange" asp-for="@constMngAgMachinery.AllocatedPointsRange" class="form-control"
                                            asp-items="@(new SelectList(Enum.GetValues(typeof(AllocatedPointRange)).Cast<AllocatedPointRange>().Select(e => new SelectListItem { Text = e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name, Value = e.ToString(), Selected = ( e.GetType().GetMember(e.ToString())[0].GetCustomAttributes(typeof(DisplayAttribute), false).OfType<DisplayAttribute>().FirstOrDefault()?.Name == Model.AllocatedPointsRange.ToString()) }), "Value", "Text"))">
                                    </select>
                                    <span asp-validation-for="AllocatedPointsRange" class="text-danger"></span>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="ModelNo">Model Number</label>
                                        <input name="ModelNo" asp-for="@constMngAgMachinery.ModelNo" type="text" value="@Model.ModelNo" class="form-control form-control-sm" placeholder="Model Number" required>
                                        <span asp-validation-for="ModelNo" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="EngineNo">Engine Number</label>
                                        <input name="EngineNo" asp-for="@constMngAgMachinery.EngineNo" type="text" value="@Model.ModelNo" class="form-control form-control-sm" placeholder="Engine Number" required>
                                        <span asp-validation-for="EngineNo" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="ChassisNo">Chassis Number</label>
                                        <input name="ChassisNo" asp-for="@constMngAgMachinery.ChassisNo" type="text" value="@Model.ChassisNo" class="form-control form-control-sm" placeholder="Chassis Number" required>
                                        <span asp-validation-for="ChassisNo" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="SerialNo">Serial Number</label>
                                        <input name="SerialNo" asp-for="@constMngAgMachinery.SerialNo" type="text" value="@Model.SerialNo" class="form-control form-control-sm" placeholder="Serial Number" required>
                                        <span asp-validation-for="SerialNo" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="PlateNo">Plate Number</label>
                                        <input name="PlateNo" asp-for="@constMngAgMachinery.PlateNo" type="text" value="@Model.PlateNo" class="form-control" placeholder="Plate Number" required>
                                        <span asp-validation-for="PlateNo" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="TDNo">TD Number</label>
                                        <input name="TDNo" asp-for="@constMngAgMachinery.TDNo" type="text" class="form-control form-control-sm" placeholder="TD Number" required>
                                        <span asp-validation-for="TDNo" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="YearOfManufacture">Year of Manufacture</label>
                                        <input name="YearOfManufacture" asp-for="@constMngAgMachinery.YearOfManufacture" value="@Model.YearOfManufacture" type="number" class="form-control form-control-sm" min="1900" max="2099" step="1" required>
                                        <span asp-validation-for="YearOfManufacture" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="Remark">Remark</label>
                                        <textarea name="Remark" asp-for="@constMngAgMachinery.Remark" class="form-control form-control-sm" placeholder="Remark" rows="3" required></textarea>
                                        <span asp-validation-for="Remark" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group form-group-sm col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="PhysicalAndInstallationAssesment">Physical and Installation Assessment</label>
                                        <textarea name="PhysicalAndInstallationAssesment" asp-for="@constMngAgMachinery.PhysicalAndInstallationAssesment" class="form-control form-control-sm" rows="3" placeholder="Physical and Installation Assessment" required></textarea>
                                        <span asp-validation-for="PhysicalAndInstallationAssesment" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-4">
                                    <div class="form-group">
                                        <label asp-for="OverallSurveyAssesment">Overall Survey Assessment</label>
                                        <textarea name="OverallSurveyAssesment" asp-for="@constMngAgMachinery.OverallSurveyAssesment" class="form-control form-control-sm" placeholder="Overall Survey Assessment" rows="3" required></textarea>
                                        <span asp-validation-for="OverallSurveyAssesment" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xl-3">
                                    <div class="form-group">
                                        <label asp-for="CountryOfOrigin">Country of Origin</label>
                                        <input name="CountryOfOrigin" asp-for="@constMngAgMachinery.CountryOfOrigin" type="text" class="form-control form-control-sm" placeholder="Select Country of Origin" list="country" required>
                                        <span asp-validation-for="CountryOfOrigin" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-3">
                                    <div class="form-group">
                                        <label asp-for="InvoiceValue">Invoice Value</label>
                                        <input name="InvoiceValue" asp-for="@constMngAgMachinery.InvoiceValue" type="number" class="form-control form-control-sm" placeholder="Invoice Value" required>
                                        <span asp-validation-for="InvoiceValue" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-3">
                                    <div class="form-group">
                                        <label asp-for="Currency">Currency</label>
                                        <input name="Currency" asp-for="@constMngAgMachinery.Currency" list="currencyList" type="text" class="form-control form-control-sm" placeholder="Select Currency" required>
                                        <span asp-validation-for="Currency" class="text-danger"></span>
                                    </div>
                                </div>
                                <div class="form-group col-xl-3">
                                    <div class="form-group">
                                        <label asp-for="ExchangeRate">Exchange Rate</label>
                                        <input name="ExchangeRate" asp-for="@constMngAgMachinery.ExchangeRate" list="currencyList" type="text" class="form-control form-control-sm" placeholder="Enter Exchange Rate" required>
                                        <span asp-validation-for="ExchangeRate" class="text-danger"></span>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group">
                                    <div class="input-group">
                                        <div class="input-group-append">
                                            <button type="submit" id="btnSaveForResubmiting" class="  btn bg-purple mr-2">Evaluate <i class="fas fa-toolbox"></i></button>
                                            <button type="button" class="btn  btn-secondary" data-dismiss="modal">Close</button>
                                           
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </section>
        </div>
    </div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("ScriptPartial/_ConstSriptsPartial");
        await Html.RenderPartialAsync("ScriptPartial/_ValidationScriptsPartial");
    }
}


