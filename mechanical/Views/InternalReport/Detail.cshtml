
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h4>Internal Report Generate</h4>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">Report Generate</li>
                </ol>
            </div>
        </div>
    </div>
</section>
<section class="content">
    <div class="container-fluid">
        <div class="card">
            <div class="card-header">
                <div class="row">
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>From Date</label>
                            <input type="date" id="fromDate" class="form-control" />
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label>To Date</label>
                            <input type="date" id="toDate" class="form-control" />
                        </div>
                    </div>
                    <div class="col-md-2 align-self-end">
                        <button id="filterBtn" class="btn btn-primary">Filter</button>
                        <button id="resetBtn" class="btn btn-default">Reset</button>
                    </div>
                </div>
            </div>
            <div class="card-body">
                <table id="MyNewCase" class="table table-bordered table-striped">
                    <thead>
                        <tr>
                            <th>Created At</th>
                            <th>Case No</th>
                            <th>Requesting Unit</th>
                            <th>Segment</th>
                            <th>Customer Name</th>
                            <th>Team Leader</th>
                            <th>Total Collateral</th>
                            <th>Status</th>
                        </tr>
                    </thead>
                     </table>
            </div>
        </div>
    </div>
</section>
@section Scripts {
    @{
        await Html.RenderPartialAsync("Partial/_PCECasesReportDatatableSriptsPartial"); 
        await Html.RenderPartialAsync("PCEScriptPartial/Partial/_CommonLibrariesScriptPartial");
    }
}


